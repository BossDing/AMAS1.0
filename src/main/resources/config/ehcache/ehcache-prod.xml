<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns='http://www.ehcache.org/v3'
         xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">

    <cache-template name="simple">
        <expiry>
            <ttl unit="seconds">3600</ttl>
        </expiry>
        <heap unit="entries">1000</heap>
    </cache-template>

    <cache alias="com.binana.amas.domain.User" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Authority" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.User.authorities" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.PersistentToken" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.User.persistentTokens" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Activity" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Amember" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Amember.associations" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Amember.departments" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Amember.roles" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Association" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Association.departments" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Association.activities" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Association.properties" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Association.membassos" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Department" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Department.membdepts" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Duty" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Duty.amembers" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Property" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Role" uses-template="simple"/>

    <cache alias="com.binana.amas.domain.Role.membroles" uses-template="simple"/>

    <!-- jhipster-needle-ehcache-add-entry -->
</config>
